      "type": "categorical"
    },
    {
      "key": "lineage",
      "title": "Lineage",
      "type": "categorical"
    },
    {
      "key": "date",
      "title": "Collection Date",
      "type": "categorical"
    },
    {
      "key": "host",
      "title": "Host",
      "type": "categorical"
    },
    {
      "key": "strain",
      "title": "Strain name",
      "type": "categorical"
    },
    {
      "key": "strain_display",
      "title": "Pretty strain name",
      "type": "categorical"
    },
    {
      "key": "region",
      "title": "Region",
      "type": "categorical"
    },
    {
      "key": "country",
      "title": "Country",
      "type": "categorical"
    }
  ],
  "display_defaults": {
    "color_by": "clade_membership",
    "map_triplicate": false,
    "distance_measure": "div",
    "tip_label": "strain_display",
    "transmission_lines": false
